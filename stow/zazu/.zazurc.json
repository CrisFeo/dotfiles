{
  "disableAnalytics": true,
  "displayOn": "detect",
  "hotkey": "cmd+space",
  "plugins": [
    {
      "name": "tinytacoteam/zazu-file-finder",
      "variables": {
        "directories": {
          "appPath": [
            "/Applications"
          ],
          "excludeName": [
            "node_modules",
            "*.scpt"
          ],
          "excludePath": [
            "~/Library"
          ],
          "filePath": [
            "~/Documents",
            "~/Downloads",
            "~/Dropbox"
          ]
        }
      }
    },
    "tinytacoteam/zazu-calculator",
    "tinytacoteam/zazu-mac-1password",
    "halvves/zazu-giphy"
  ],
  "theme": "singuerinc/zazu-clean-dark-theme"
}
